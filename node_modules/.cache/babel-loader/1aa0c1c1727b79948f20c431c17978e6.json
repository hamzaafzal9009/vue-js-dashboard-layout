AA,SAAA,EAAA,MAAA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,WAAA,EAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA;AACA,QAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,YAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,QAAA,EAAA,CAAA,UAAA,GAAA,KAAA;AACA,OAdA;AAeA,KApBA;AAqBA,IAAA,QArBA,sBAqBA;AACA,UAAA,MAAA,GAAA,CACA,MAAA,CAAA,MAAA,CAAA,WAAA,GAAA,MAAA,CAAA,gBADA,EAEA,MAAA,CAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,gBAFA,CAAA;;AAIA,UAAA,MAAA,CAAA,CAAA,CAAA,GAAA,IAAA,IAAA,MAAA,CAAA,CAAA,CAAA,GAAA,GAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA,KA/BA;AAgCA,IAAA,SAhCA,uBAgCA;AACA,UAAA,IAAA,GAAA,KAAA,EAAA,GAAA,EAAA,GAAA,IAAA,GAAA,GAAA;AACA,UAAA,GAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,OAAA,KAAA,IAAA;;AACA,UAAA,KAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,MAAA,mBAAA,KAAA,KAAA,uBAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,QAAA,QAAA,CAAA,MAAA,oCAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA;;AACA,UAAA,CAAA,KAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,MAAA,6BAAA,IAAA,IAAA,CAAA,GAAA,CAAA,OAAA,KAAA,IAAA,CAAA;AACA,QAAA,QAAA,CAAA,MAAA;AACA;AACA,KA5CA;AA6CA,IAAA,OA7CA,qBA6CA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,wBAAA;AACA;AA/CA;AAfA,CAAA","sourcesContent":["<template>\n  <div class=\"login-wrapper\">\n    <div class=\"content\">\n      <div class=\"header\">\n        <img\n          class=\"header-image\"\n          src=\"../assets/img/logo-transparent.png\"\n          width=\"100\"\n          alt=\"Opiniion\"\n        />\n        <div class=\"header-link-wrapper\">\n          <el-link\n            class=\"header-link\"\n            href=\"http://viteadmin.site/\"\n            type=\"#FFFFFF\"\n            target=\"_blank\"\n            icon=\"fa fa-angle-left\"\n            >&nbsp;&nbsp;Go to viteadmin.site</el-link\n          >\n        </div>\n      </div>\n      <el-card class=\"absolute-centered login-card\">\n        <span class=\"login-header horizontal-centered\">Login</span>\n        <el-input\n          v-model=\"email\"\n          prefix-icon=\"fal fa-envelope\"\n          placeholder=\"Email\"\n          @keyup.enter.native=\"doLogin()\"\n        ></el-input>\n        <el-input\n          v-model=\"password\"\n          prefix-icon=\"fal fa-key\"\n          placeholder=\"Password\"\n          type=\"password\"\n          @keyup.enter.native=\"doLogin()\"\n          show-password\n        ></el-input>\n        <div class=\"error-message\" v-if=\"loginError\">{{ loginError }}</div>\n        <div class=\"remember-container flex-space-between\">\n          <el-checkbox :checked=\"remember\" v-model=\"remember\"\n            >Remember Me</el-checkbox\n          >\n          <router-link to=\"/\" class=\"fp float-right\" href\n            >Forgot Password?</router-link\n          >\n        </div>\n\n        <el-button class=\"login-button\" @click=\"doLogin\" type=\"primary\" round\n          >Login</el-button\n        >\n      </el-card>\n    </div>\n    <footer class=\"footer\">\n      <div class=\"login-bottom-nav flex-center\">\n        <div>\n          <div class=\"credits ml-auto\">\n            <div class=\"copyright\">\n              &copy; {{ new Date().getFullYear() }} Viteapp. All Rights Reserved\n            </div>\n          </div>\n        </div>\n      </div>\n    </footer>\n  </div>\n</template>\n\n<script>\nimport { EventService } from \"../main.js\";\nimport firebase from \"firebase/app\";\nimport \"firebase/auth\";\n\nexport default {\n  name: \"login\",\n  data: function() {\n    return {\n      email: \"\",\n      password: \"\",\n      loginError: null,\n      logoutReason: null,\n      remember: false,\n    };\n  },\n  created() {\n    this.logoutReason = this.$route.params.reason;\n    \n  },\n  methods: {\n    doLogin() {\n      var vm = this;\n      EventService.$emit(\"loading\", \"show\");\n      vm.loginError = null;\n      firebase\n        .auth()\n        .signInWithEmailAndPassword(\n          this.email,\n          this.password\n        )\n        .then((response) => {\n          EventService.$emit(\"loading\", \"hide\");\n          localStorage.setItem(\"authAdmin\", JSON.stringify(response));\n          vm.$router.push(\"/dashboard\");\n        })\n        .catch((error) => {\n          EventService.$emit(\"loading\", \"hide\");\n          vm.loginError = error;\n        });\n    },\n    isMobile() {\n      let screen = [\n        window.screen.availHeight * window.devicePixelRatio,\n        window.screen.availWidth * window.devicePixelRatio,\n      ];\n      if (screen[0] < 1000 && screen[1] < 800) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    setCookie() {\n      let year = 60 * 60 * 24 * 1000 * 365;\n      let now = new Date();\n      let then = now.getTime() + year;\n      if (this.remember) {\n        document.cookie = `email=${this.email}; expires=${new Date(then)};`;\n        document.cookie = `remember=true; expires=${new Date(then)};`;\n      }\n      if (!this.remember) {\n        document.cookie = `email=; expires=${new Date(now.getTime() - year)};`;\n        document.cookie = `remember=; expires=-1;`;\n      }\n    },\n    goToWWW() {\n      window.location.href = \"http://viteadmin.site/\";\n    },\n  },\n};\n</script>\n\n<style scoped>\n@media (max-width: 600px) {\n  .header-image {\n    margin: auto;\n  }\n  .header-link-wrapper {\n    margin: 10px auto;\n  }\n}\n.login-wrapper {\n  height: 100vh;\n  width: 100%;\n  background: url(\"../assets/img/login-bg.svg\");\n  background-size: cover;\n  background-repeat: no-repeat;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  flex-wrap: wrap;\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  box-sizing: border-box;\n  height: 130px;\n  padding: 40px;\n}\n\n.header-link {\n  color: white;\n}\n.header > .header-row {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin: 10px 25px;\n}\n\n.header-row > img {\n  width: 170px;\n}\n\n.left-header {\n  font-size: 15px !important;\n  color: white;\n  z-index: 999;\n}\n\n.login-button {\n  width: 100%;\n  margin-top: 10px;\n}\n.login-card {\n  animation: 0.35s ease slideFadeFromBottom;\n  position: relative;\n  width: 340px;\n  max-width: 90%;\n  height: 380px;\n}\n\n.content {\n  width: 100vw;\n  height: 100vh;\n  margin: auto;\n  z-index: 10;\n  position: absolute;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  top: 0;\n  margin: auto;\n  display: flex;\n}\n\n.login-header {\n  margin: 0 0 40px 0;\n  font-size: 30px;\n}\n\n.remember-container {\n  margin: 20px 0px 20px 0px;\n}\n\n.fp {\n  text-decoration: none;\n  color: 1976d2 !important;\n  font-size: 12px;\n}\n\n.agreement-container {\n  font-size: 12px;\n  width: 80%;\n  text-align: center;\n  margin: 20px auto;\n  line-height: 1.5;\n}\n\nfooter {\n  position: absolute;\n  width: 100%;\n  bottom: 10px;\n  font-size: 12px;\n  color: #757575;\n}\n\n.login-bottom-nav {\n  margin: 30px 50px;\n}\n\n.router-links {\n  width: 200px;\n}\n\n.fc {\n  color: white;\n  text-decoration: none;\n}\n\n.fa-angle-left {\n  margin-right: 8px;\n}\n\n.error-message {\n  color: #dd2c00;\n  width: 100%;\n  font-size: 11px;\n  text-align: left;\n  margin-bottom: 5px;\n}\n@media (max-width: 360px) {\n  .header {\n    margin: 0;\n    padding: 10px;\n  }\n}\n@media (max-width: 1200px) {\n  .header {\n    margin-top: 20px;\n  }\n  .login-wrapper {\n    background: url(\"../assets/img/login-bg.svg\") 50% 50%;\n  }\n}\n@media screen and (max-width: 500px) {\n  .header > .header-row {\n    flex-direction: column;\n    justify-content: center;\n  }\n  .header > .header-row > div {\n    margin-top: 10px;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}